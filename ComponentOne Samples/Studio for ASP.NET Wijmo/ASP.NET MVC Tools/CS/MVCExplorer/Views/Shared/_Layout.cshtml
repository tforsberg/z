<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <!--Theme-->
    @{
        if (Session["theme"] == null)
        { 
        <link class="wijmo-stylesheet-wijmo_theme" href="http://cdn.wijmo.com/themes/rocket/jquery-wijmo.css" rel="stylesheet"
            type="text/css" title="rocket-jqueryui" />    
        }
        else
        { 
        <link class="wijmo-stylesheet-wijmo_theme" href="@Session["theme"]" rel="stylesheet" type="text/css" title="rocket-jqueryui" /> 
        }
    }
    <!--Explorer CSS-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/explore/css/style.css")" />
    <!--jQuery References-->
    <script type="text/javascript" src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
    <script type="text/javascript" src="http://code.jquery.com/ui/1.10.2/jquery-ui.min.js"></script>
    <!--Wijmo Widgets CSS-->
    <link href="http://cdn.wijmo.com/jquery.wijmo-pro.all.3.20133.19.min.css" rel="stylesheet"
        type="text/css" />
    <!--Sample Dependencies-->
    <script src="http://cdn.wijmo.com/external/amplify.core.min.js" type="text/javascript"></script>
    <script src="http://cdn.wijmo.com/external/amplify.store.min.js" type="text/javascript"></script>
    <script src="http://cdn.wijmo.com/external/jquery.cookie.js" type="text/javascript"></script>
    <script src="http://cdn.wijmo.com/external/jquery.tmpl.min.js" type="text/javascript"></script>
    <script src="http://cdn.wijmo.com/external/swfobject.js" type="text/javascript"></script>
    <!--Wijmo Widgets JavaScript-->
    <script type="text/javascript" src="http://cdn.wijmo.com/jquery.wijmo-open.all.3.20133.19.min.js"></script>
    <script type="text/javascript" src="http://cdn.wijmo.com/jquery.wijmo-pro.all.3.20133.19.min.js"></script>
    <script src="http://cdn.wijmo.com/external/cultures/globalize.cultures.js" type="text/javascript"></script>
    <!--SyntaxHighlighter-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/explore/css/shCoreDefault.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/explore/css/shThemeMidnight.css")" />
    <script type="text/javascript" src="@Url.Content("~/explore/js/shCore.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/explore/js/shBrushJScript.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/explore/js/shBrushXml.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/explore/js/shBrushCss.js")"></script>
    @RenderSection("header", false)
</head>
<body>
    <div class="wrapper">
        <header>
            <div class="branding ui-helper-clearfix">
                <h1 id="logo">
                    <a href="@Url.Content("~/")" runat="server">ASP.NET Wijmo MVC Explorer</a></h1>
                <div id="button-container">
                    <a class="downloadlink" href="http://www.componentone.com/StudioASPNET/Download/"><span class="downloadlink-inner"><span class="download-get">Download</span> <span class="download-wijmo">Free Trial</span> </span></a>
                </div>
            </div>
            <div class="title-bar ui-helper-clearfix">
                <div class="menu-container">
                    <div class="menu-extender">
                    </div>
                    <div class="padder">
                       @RenderSection("HeaderButtons", false)
                    </div>
                </div>
                <div class="ribbon">
                    <div class="ribbon-right">
                    </div>
                    <h2>
                    @RenderSection("HeaderTitle", false)
                    </h2>
                </div>
            </div>
        </header>
        @RenderBody()
        @RenderSection("description", false)
        <footer class="ui-helper-clearfix" id="colophon">
            <p>
               <img runat="server" src=@Url.Content("~/explore/css/images/c1logo.png") />
                © 2013 GrapeCity, Inc. All Rights Reserved. All product and company names herein may be trademarks of their respective owners.
            </p>
            <div class="social">
                <a href="http://twitter.com/wijmo">
                    <img runat="server" src=@Url.Content("~/explore/css/images/icons/32/picons33.png") alt="Twitter" /></a> <a href="http://facebook.com/wijmo">
                        <img runat="server" src=@Url.Content("~/explore/css/images/icons/32/picons36.png") alt="Twitter" /></a>
            </div>
        </footer>
    </div>
    <script type="text/javascript" src="@Url.Content("~/explore/js/init.js")"></script>
</body>
</html>
