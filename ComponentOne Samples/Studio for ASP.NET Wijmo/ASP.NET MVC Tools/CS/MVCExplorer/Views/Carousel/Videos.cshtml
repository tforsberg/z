@{
    ViewBag.Title = "Videos";
    Layout = "~/Views/Shared/_WidgetLayout.cshtml";
}
@section header
{
    <script id="scriptInit" type="text/javascript">
        $(document).ready(function () {
            $("#wijcarousel0").wijcarousel({
                showContorlsOnHover: true
            });
        });
    </script>
    <style type="text/css">
        #wijcarousel0
        {
            width: 600px;
            height: 338px;
        }
    </style>
}
<div class="main demo">
